{
  "name": "teatime-test-utils",
  "version": "0.6.8",
  "description": "Teatime test utils",
  "main": "preset.js",
  "engines": {
    "node": ">=6.3"
  },
  "scripts": {
    "docker:build": "docker build -t $npm_package_name:$npm_package_version env",
    "docker:run": "docker run --tty --interactive --volume $(pwd):/var/www/test $npm_package_name:$npm_package_version",
    "test": "npm run test:unit",
    "test:unit": "ava --require ./preset test/*/*.js"
  },
  "author": "Alexey Litvinov",
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-preset-power-assert": "^1.0.0",
    "babel-register": "^6.11.6",
    "static-server": "^2.0.3",
    "teatime-components": "^0.6.6",
    "webdriverio": "^4.2.3"
  },
  "dependencies": {
    "css-modules-require-hook": "^4.0.1",
    "lodash": "^4.14.1",
    "power-assert": "^1.4.1"
  }
}
